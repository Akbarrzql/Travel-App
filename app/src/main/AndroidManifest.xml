<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.travelapps" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TravelApps" >
        <activity
            android:name=".Main.NotificationActivity"
            android:exported="false" />
        <activity
            android:name=".Main.InboxActivity"
            android:exported="false" />
        <activity
            android:name=".Main.Splash_Screen_Activity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Detail.DetailNews"
            android:parentActivityName=".Detail.DetailNews" />
        <activity
            android:name=".Detail.DetailView"
            android:parentActivityName=".Detail.DetailView" />
        <activity
            android:name=".Main.MainActivity"
            android:exported="true" />
    </application>

</manifest>